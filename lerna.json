{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": [
    "packages/*"
  ],
  "command": {
    "version": {
      "exact": true
    },
    "publish": {
      "npmClient": "npm",
      "allowBranch": [
        "master",
        "canary"
      ],
      "registry": "https://registry.npmjs.org/"
    }
  },
  "changelog": {
    "repo": "boyney123/eventcatalog",
    "changelog": {
      "labels": {
        "Type: Feature": "Features",
        "Type: Bug": "Bug fixes",
        "Type: Enhancement": "Enhancements"
      }
    }
  },
  "version": "0.0.2"
}
