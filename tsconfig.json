{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@config": ["./eventcatalog.config.js"],
      "@eventcatalog": ["src/utils/config/catalog.ts"],
      "@components/*": ["src/components/*"],
      "@types": ["src/types/index.ts"],
      "@utils/*": ["src/utils/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["vitest/globals"]
  }
}
