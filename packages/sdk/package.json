{
  "name": "@eventcatalog/sdk",
  "version": "2.12.1",
  "description": "SDK to integrate with EventCatalog",
  "scripts": {
    "build": "tsup",
    "build:bin": "tsup",
    "test": "vitest --run",
    "test:ci": "vitest --run",
    "format": "prettier --write .",
    "format:diff": "prettier --list-different ."
  },
  "bin": {
    "eventcatalog": "./dist/cli/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.10",
    "@types/proper-lockfile": "^4.1.4",
    "@types/semver": "^7.5.8",
    "prettier": "^3.3.3",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": [
        "./dist/index.d.ts",
        "./dist/events.t.ts"
      ],
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./cli-docs": {
      "types": "./dist/cli-docs.d.ts",
      "require": "./dist/cli-docs.js",
      "import": "./dist/cli-docs.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@changesets/cli": "^2.29.8",
    "commander": "^12.0.0",
    "fs-extra": "^11.3.2",
    "glob": "^11.1.0",
    "gray-matter": "^4.0.3",
    "proper-lockfile": "^4.1.2",
    "semver": "7.7.3",
    "slugify": "^1.6.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/event-catalog/eventcatalog"
  }
}
