{
  "name": "@eventcatalog/visualiser",
  "version": "3.12.9-beta.1",
  "description": "ReactFlow nodes and visualiser components for EventCatalog - framework agnostic",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && pnpm build:css",
    "build:bin": "tsup && pnpm build:css",
    "build:css": "postcss src/styles.css -o dist/styles.css",
    "build:watch": "concurrently \"tsup --watch\" \"postcss src/styles.css -o dist/styles.css --watch\"",
    "dev": "tsup --watch --onSuccess \"pnpm build:css\"",
    "playground": "vite playground --config playground/vite.config.ts",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:diff": "prettier --check \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "eventcatalog",
    "reactflow",
    "visualization",
    "nodes",
    "event-driven-architecture"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/event-catalog/eventcatalog.git"
  },
  "author": "EventCatalog",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@xyflow/react": "^12.6.1",
    "dagre": "^0.8.5",
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.510.0",
    "marked": "^16.1.2",
    "mermaid": "^11.4.1",
    "svg-pan-zoom": "^3.6.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "@types/dagre": "^0.7.53",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.5",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vite": "^7.1.9"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
